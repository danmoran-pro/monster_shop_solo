<h1>Create New Coupon for Your Shop </h1>
<center>
  <%= form_tag "/merchant/coupons", method: :post do %>
    <%= label_tag :name %>
    <%= text_field_tag :name%>

    <%= label_tag :code %>
    <%= text_field_tag :code %>

    <%= label_tag :percentage_off %>
    <%= text_field_tag :percentage_off %>

    <%= submit_tag 'Submit' %>
  <% end %>
</center>
